<extensions>
   <page id="tutorials">
      <group path="page-content/bottom-left" default="true">
      </group>
      <group path="page-content/bottom-right" default="true">
      </group>
      <group path="page-content/top-left">
         <extension id="org.eclipse.jdt.2" importance="low"/>
         <extension id="org.eclipse.team.2" importance="low"/>
      </group>
      <group path="page-content/top-right">
         <extension id="org.eclipse.pde.2" importance="low"/>
      </group>
   </page>
   <page id="whatsnew">
      <group path="page-content/bottom-left" default="true">
      </group>
      <group path="page-content/bottom-right" default="true">
      </group>
      <group path="page-content/top-left">
         <extension id="org.eclipse.ui.workbench.news.2" importance="low"/>
         <extension id="org.eclipse.jdt.2" importance="low"/>
         <extension id="org.eclipse.pde.changes.2" importance="low"/>
         <extension id="org.eclipse.ui.workbench.migration.2" importance="callout"/>
      </group>
      <group path="page-content/top-right">
         <extension id="org.eclipse.ui.workbench.2" importance="low"/>
      </group>
   </page>
   <page id="samples">
      <group path="page-content/bottom-left" default="true">
      </group>
      <group path="page-content/bottom-right" default="true">
      </group>
      <group path="page-content/top-left">
         <extension id="org.eclipse.pde.workbench.2" importance="low"/>
      </group>
      <group path="page-content/top-right">
         <extension id="org.eclipse.jdt.2" importance="low"/>
         <extension id="org.eclipse.pde.swt.2" importance="low"/>
      </group>
   </page>
   <page id="overview">
      <group path="page-content/bottom-left" default="true">
      </group>
      <group path="page-content/bottom-right" default="true">
      </group>
      <group path="page-content/top-left">
         <extension id="org.eclipse.ui.workbench.2" importance="low"/>
      </group>
      <group path="page-content/top-right">
         <extension id="org.eclipse.jdt.2" importance="low"/>
         <extension id="org.eclipse.pde.2" importance="low"/>
      </group>
   </page>
</extensions>
